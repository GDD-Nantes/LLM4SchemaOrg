[{"author": {"url": "https://www.southernliving.com/author/the-sl-test-kitchen", "@type": "Person", "description": "The Southern Living Test Kitchen has been publishing recipes since 1970, four years after the first issue of Southern Living Magazine appeared on newsstands. The Southern Living Test Kitchen team includes a team of professionals with deep expertise in recipe development, from pastry chefs and grilling experts to nutritionists and dietitians. Together, the team tests and retests, produces, styles, and photographs thousands of recipes each year in the state-of-the-art test kitchen facility located in Birmingham, Alabama."}, "publisher": {"@type": "Organization", "logo": {"height": 111, "@type": "ImageObject", "width": 640, "url": "https://www.southernliving.com/thmb/ESKPvTBAXaIuvqPKIXBHM-S4bSM=/640x111/filters:no_upscale():max_bytes(150000):strip_icc()/brand_logo_schema3-99a42e67d7c7496cacfce4cde9c2e6b9.jpg"}, "name": "Southern Living", "url": "https://www.southernliving.com", "brand": "Southern Living", "sameAs": []}, "name": null, "mainEntityOfPage": "https://www.southernliving.com/recipes/cauliflower-rice-bowl-with-tahini-herb-chicken", "description": "Colorful vegetables and grilled chicken team up in this simple yet satisfying meal. The cauliflower rice bowl is a low-carb meal that the whole family will enjoy.", "datePublished": "2022-08-19T22:32:07.000-04:00", "image": [{"height": 1500, "@type": "ImageObject", "width": 2000}, {"width": 1500, "height": 1500, "@type": "ImageObject"}, {"width": 2000, "height": 1125, "@type": "ImageObject"}], "@type": ["NewsArticle", "Recipe"], "recipeIngredient": [], "dateModified": "2022-08-19T22:32:07.000-04:00", "@context": "http://schema.org"}]