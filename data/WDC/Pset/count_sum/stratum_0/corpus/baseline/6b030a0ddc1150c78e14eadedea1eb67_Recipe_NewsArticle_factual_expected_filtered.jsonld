[{"publisher": {"@type": "Organization", "logo": {"@type": "ImageObject", "url": "https://www.southernliving.com/thmb/ESKPvTBAXaIuvqPKIXBHM-S4bSM=/640x111/filters:no_upscale():max_bytes(150000):strip_icc()/brand_logo_schema3-99a42e67d7c7496cacfce4cde9c2e6b9.jpg", "width": 640, "height": 111}, "url": "https://www.southernliving.com", "name": "Southern Living", "brand": "Southern Living", "sameAs": []}, "datePublished": "2022-08-19T22:32:07.000-04:00", "description": "Colorful vegetables and grilled chicken team up in this simple yet satisfying meal. The cauliflower rice bowl is a low-carb meal that the whole family will enjoy.", "image": [{"height": 1500, "@type": "ImageObject", "url": "https://www.southernliving.com/thmb/1xKZnBUnNIqK9p22I4NGlQNyqQk=/2000x1500/smart/filters:no_upscale()/27162_FGFsuperbowl_0359_4x3-2000-0d9e3c72fed441849ad7773bc9909306.jpg", "width": 2000}, {"url": "https://www.southernliving.com/thmb/faZWSOHhAMOAgiDggWq4PuinCCM=/1500x1500/smart/filters:no_upscale()/27162_FGFsuperbowl_0359_4x3-2000-0d9e3c72fed441849ad7773bc9909306.jpg", "height": 1500, "width": 1500, "@type": "ImageObject"}, {"height": 1125, "width": 2000, "url": "https://www.southernliving.com/thmb/5PmesfhQMxu_QtXpbBUQ4iGkXj8=/2000x1125/smart/filters:no_upscale()/27162_FGFsuperbowl_0359_4x3-2000-0d9e3c72fed441849ad7773bc9909306.jpg", "@type": "ImageObject"}], "recipeYield": null, "name": null, "totalTime": "PT220M", "@type": ["Recipe", "NewsArticle"], "headline": null, "nutrition": {"fatContent": "26 g", "@type": "NutritionInformation", "unsaturatedFatContent": "0 g"}, "dateModified": "2022-08-19T22:32:07.000-04:00", "mainEntityOfPage": "https://www.southernliving.com/recipes/cauliflower-rice-bowl-with-tahini-herb-chicken", "recipeInstructions": [{"@type": "HowToStep", "text": "Preheat grill to medium-high (400°F to 450°F). Combine onion, olive oil, and remaining 1/4 teaspoon Greek seasoning in a small bowl; toss to coat. Place onion wedges on oiled grates. Remove chicken thighs from bag, discarding yogurt marinade. Place chicken on oiled grates. Grill onion wedges, uncovered, turning often, until charred and tender, 10 to 15 minutes. Grill chicken thighs, uncovered, until cooked through and an instant-read thermometer inserted in thickest part of thigh registers 165°F, 5 to 7 minutes per side. Remove onion wedges and chicken from grill, and let rest 5 minutes. Cut 2 of the chicken thighs in half."}, {"@type": "HowToStep", "text": "Process yogurt, parsley, tahini, dill, garlic, lemon zest and juice, cayenne pepper, and 1/2 teaspoon of the Greek seasoning in a high-powered blender or food processor until smooth, about 45 seconds."}, {"text": "Divide riced cauliflower evenly among 4 bowls. Top evenly with chickpeas, cucumber, halved cherry tomatoes, grilled onion wedges, kalamata olives, and chicken. Top each bowl with about 3 tablespoons of remaining yogurt dressing. Garnish with additional fresh parsley or dill.", "@type": "HowToStep"}, {"text": "Combine half of yogurt dressing and chicken thighs in a large ziplock plastic bag. Seal bag, massage yogurt dressing into chicken, and marinate in refrigerator at least 3 hours or up to 12 hours. Refrigerate remaining yogurt dressing in an airtight container until ready to serve.", "@type": "HowToStep"}, {"@type": "HowToStep", "text": "Prepare riced cauliflower according to package instructions."}], "author": {"@type": "Person", "url": "https://www.southernliving.com/author/the-sl-test-kitchen", "description": "The Southern Living Test Kitchen has been publishing recipes since 1970, four years after the first issue of Southern Living Magazine appeared on newsstands. The Southern Living Test Kitchen team includes a team of professionals with deep expertise in recipe development, from pastry chefs and grilling experts to nutritionists and dietitians. Together, the team tests and retests, produces, styles, and photographs thousands of recipes each year in the state-of-the-art test kitchen facility located in Birmingham, Alabama."}, "recipeIngredient": ["2 lb. boneless, skinless chicken thighs (about 6 thighs)", "0.75 cup packed fresh flat-leaf parsley leaves and tender stems, plus more for garnish", "1 (15 1/2-oz.) can no-salt-added chickpeas (garbanzo beans), drained and rinsed", "0.25 cup tahini (sesame paste)", "0.5 tsp. grated lemon zest plus 1/4 cup fresh juice (from 2 lemons)", "0.25 cup pitted kalamata olives", "1 small (9 oz.) cucumber, diced (about 1 cup)", "1 pt. multicolored cherry tomatoes, halved (about 2 cups)", "1 (10-oz.) pkg. fresh or frozen riced cauliflower (about 1 1/2 cups)", "1 cup plain 2% reduced-fat strained yogurt (such as Greek)", "1 tsp. olive oil", "0.5 medium (10 oz.) red onion, cut into 1-in. wedges with root intact (about 1 cup)", "2 Tbsp. fresh dill (from 1 [1/2-oz.] pkg.), plus more for garnish", "0.75 tsp. Greek seasoning, divided"], "@context": "http://schema.org"}]