[{"@type": "MedicalProcedure", "description": "Contact Lenses", "@context": "http://schema.org"}, {"@type": "MedicalProcedure", "description": "Keratoconus", "@context": "http://schema.org"}, {"alternateName": "KAMRA® Inlay", "@type": "MedicalProcedure", "howPerformed": "A laser creates a pocket in the first few layers of the cornea and then the lens is placed in the pocket and centered over the pupil. Only one eye is treated.", "description": "KAMRA®", "@context": "http://schema.org"}, {"disambiguatingDescription": "Visian ICL™ is an implantable, biocompatible lens that corrects moderate to high nearsightedness for patients who don\\'t want to wear contact lenses or eyeglasses.", "@type": "MedicalProcedure", "description": "Visian ICL™", "@context": "http://schema.org"}, {"description": "Advanced Lens Implants", "procedureType": "SurgicalProcedure", "@type": "MedicalProcedure", "@context": "http://schema.org"}, {"alternateName": "CL", "description": "Corneal Collagen Crosslinking (CXL)", "@type": "MedicalProcedure", "disambiguatingDescription": "Corneal collagen crosslinking is a minimally invasive procedure that treats keratoconus, a condition in which the cornea gets progressively thinner and weaker.", "howPerformed": "After applying specialized vitamin B-12 eye drops, the doctor shines a specific type of ultraviolet light directly onto your cornea to activate the eye drops and trigger the crosslinking.", "@context": "http://schema.org"}, {"@type": "MedicalProcedure", "disambiguatingDescription": "Photorefractive Keratectomy (PRK), is a precise, minimally invasive method of reshaping the cornea. This helps the eye to focus properly, thus reducing or eliminating the need for eyeglasses and contact lenses. ", "howPerformed": "The thin, top layer of the cornea known as epithelium is gently removed. The excimer laser is then used to reshape the cornea.", "alternateName": "Photorefractive Keratectomy", "@context": "http://schema.org"}, {"@type": "MedicalProcedure", "description": "Keratoconus", "@context": "http://schema.org"}, {"description": "Laser Cataract Surgery", "@type": "MedicalProcedure", "alternateName": "Cataract Surgery", "howPerformed": "The clouded lens is removed, and a clear artificial lens is usually implanted. In some cases, however, a cataract may be removed without implanting an artificial lens.", "@context": "http://schema.org"}, {"@type": "MedicalProcedure", "howPerformed": "The surgeon creates a tunnel between layers of the corneal stroma and inserts thin, disc-shaped implants to flatten the cornea.", "@context": "http://schema.org"}, {"howPerformed": "A thin flap in the cornea is created using either a microkeratome blade or a femtosecond laser. The surgeon folds back the flap, then removes some corneal tissue underneath using an excimer laser.", "alternateName": "Laser Eye Surgery", "@type": "MedicalProcedure", "description": "LASIK", "@context": "http://schema.org"}, {"howPerformed": "An eye doctor replaces a clear lens and replaces it with an artificial intraocular lens (IOL) to correct for age-related vision issues.", "@type": "MedicalProcedure", "disambiguatingDescription": "Refractive lens exchange (RLE) replaces your eye\\'s clear natural lens with an artificial intraocular lens (IOL) to correct your refractive error and achieve sharper focus, reducing your need for reading glasses or bifocals.", "alternateName": "Lens Replacement", "@context": "http://schema.org"}, {"@type": "MedicalProcedure", "description": "Corneal Transplant", "procedureType": "SurgicalProcedure", "howPerformed": "A section of tissue is removed from the diseased or injured cornea using either a surgical cutting instrument called a trephine or a femtosecond laser. The surgeon then sutures donor tissue into place.", "disambiguatingDescription": "A cornea transplant is a surgical procedure that replaces diseased or scarred corneal tissue with healthy tissue from an organ donor.", "@context": "http://schema.org"}]